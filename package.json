{
  "name": "back",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "prod": "node --enable-source-maps  node_modules/fastify-cli/cli.js start dist/app.js",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "5.0.0",
    "@fastify/sensible": "5.0.0",
    "@fastify/static": "^6.6.0",
    "@types/imap": "0.8.35",
    "@types/jsdom": "^20.0.1",
    "@types/nodemailer": "6.4.7",
    "@types/uuid": "9.0.0",
    "cheerio": "^1.0.0-rc.12",
    "fastify": "^4.11.0",
    "fastify-cli": "5.7.0",
    "fastify-plugin": "4.0.0",
    "ical.js": "^1.5.0",
    "imap": "^0.8.19",
    "jquery": "^3.6.3",
    "jquery-jsdom": "^1.0.0",
    "jsel": "1.1.6",
    "json5": "^2.2.3",
    "nodemailer": "^6.8.0",
    "rtm-api": "1.3.1",
    "tslog": "^4.7.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "simple-tjscli": "^1.2.0",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
